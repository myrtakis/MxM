{
  "datasets": {
    "datasets/breast_lof_031_010.csv": {
      "target": "is_anomaly"
    }
  },

  "variable_selection": {
    "none": {},

    "ses": {
      "params": {
        "maxk": [2, 3, 4, 5],
        "alpha": [0.05, 0.1, 0.2, 0.5]
       }
    }
  },

  "classifiers": {
    "rf": {
      "params": {
        "n_estimators": [100, 300, 500, 1000],
        "min_samples_leaf": [1, 2, 3, 4, 5],
        "criterion": "entropy"
      }
    },

    "iforest": {
      "params": {
        "n_estimators": [100, 300, 500, 1000],
        "max_samples": [64, 128, 256]
      }
    }
  }

}